{
  "name": "waitlist",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "client": "astro dev",
    "build:client": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "pnpm exec prettier . --write",
    "format:check": "pnpm exec prettier . --check",
    "lint": "eslint 'src/**/*.{js,ts,tsx,astro}'",
    "worker": "wrangler dev",
    "dev": "concurrently \"pnpm run client\" \"pnpm run worker\"",
    "dryrun:staging": "pnpm run build:client && wrangler deploy --dry-run --env staging",
    "dryrun:prod": "pnpm run build:client && wrangler deploy --dry-run --env production",
    "deploy:staging": "pnpm run build:client && wrangler deploy --env staging",
    "deploy:prod": "pnpm run build:client && wrangler deploy --env production"
  },
  "dependencies": {
    "@astrojs/react": "^4.4.2",
    "@tailwindcss/vite": "^4.1.17",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "astro": "^5.15.7",
    "concurrently": "^9.2.1",
    "hono": "^4.10.7",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.17",
    "wrangler": "^4.51.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.47.0",
    "daisyui": "^5.5.4",
    "eslint": "^9.39.1",
    "eslint-plugin-astro": "^1.5.0",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.7.1"
  }
}
